<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.manuel.harmony">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Green">
        <activity
            android:name=".Home.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.Green">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Logbook.LogbookActivity"
            android:label="@string/title_activity_logbook"/>
        <activity
            android:name=".Climb.ClimbActivity"
            android:label="@string/title_activity_climb"></activity>
        <activity
            android:name=".Friends.FriendsActivity"
            android:label="@string/title_activity_friends"></activity>
        <activity
            android:name=".BaseActivity"
            android:label="@string/title_activity_base"></activity>

        <activity android:name=".tour.IntroActivity"
            android:label="@string/title_activity_intro"></activity>

    </application>

</manifest>